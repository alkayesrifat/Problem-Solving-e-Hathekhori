// Problem Link : https://dimikoj.com/problems/3/descending-number
// Video Link : https://youtu.be/5n7fVtmbnBo


#include <stdio.h>

int main(){

    int cnt = 0;

    for (int i = 1000 ; i >= 1 ; i--)
    {
        printf("%d\t",i);   cnt++;

        if (cnt % 5 == 0)
        {
            printf("\n");
        }
        
    }
    
    

    return 0;
}
