// Problem Link : https://dimikoj.com/problems/2/even-odd-2
// Video Link : https://youtu.be/GOajLBl6yRs

 #include <stdio.h>

 int main() {
 
     int test_case;
     scanf("%d",&test_case);
 
    
 
     while (test_case--)
     {
         char digits[102];
         scanf("%s",&digits);
         int size_of_digits = strlen(digits);
         char last_digit = digits[size_of_digits - 1];
         int last_number = last_digit - '0';
         if (last_number % 2 == 0)
         {
             printf("even\n");
         }
         else
         {
             printf("odd\n");
         }
         
         
     }
     
     
     return 0;
 }
